<view style='display: {{ access == "false" ? "block" : "none" }}'>
  <view class='icon-center text-muted-a'>
    <van-icon name="location"/>
    <text class='icon-text-center text-muted'>您未开通相关权限，部分功能无法使用哦 ,请先开通权限~</text>
    <van-button type="success" size='small' bindtap='accessLocation'>开通权限 </van-button>
  </view>
</view>
<view style='display: {{ access == "true" ? "block" : "none" }}'>
  <van-nav-bar fixed border="false">
    <view slot="left" style="background: {{ color.success }};padding-left:0.80rem;padding-bottom:0.50rem" class='text-default text-small'>
     <view class='ell-super'><van-icon name="location" bindtap='openLocation'/><text bindtap='openLocation'> {{ selected_location ? selected_location.title : location.title ? location.title : location.formatted_addresses.recommend }} </text> <van-icon bindtap='openLocation' name="arrow-down"/></view>
    </view>
  </van-nav-bar>
<van-search bind:tap="openSearch" disabled use-action-slot input-align="center" value="{{ value }}" placeholder="草莓、香蕉、龙眼">
  <!-- <van-icon slot="action" class='text-lg text-muted' bind:tap='openScan' name="scan" style='padding-top: 15rpx;'/> -->
</van-search>
<van-tabs border="{{ false }}" color="{{ color.success }}" active="{{ active }}"  bind:change="onChangeHomeTab">
